{
    "name": "resolve-css",
    "version": "1.0.1",
    "description": "This Package resolves the local or node_module path of the css file.",
    "main": "./cjs/index.js",
    "module": "./esm/index.js",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "rollup -c",
        "dev": "rollup -c -w",
        "test": "ava",
        "prepare": "npm run build && npm test"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/UpperCod/resolve-css.git"
    },
    "author": "UpperCod",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/UpperCod/resolve-css/issues"
    },
    "homepage": "https://github.com/UpperCod/resolve-css#readme",
    "dependencies": {
        "@uppercod/cache": "^1.0.0"
    },
    "devDependencies": {
        "@rollup/plugin-node-resolve": "^8.1.0",
        "ava": "^3.10.1",
        "builtin-modules": "^3.1.0",
        "bulma": "^0.9.0",
        "esm": "^3.2.25",
        "rollup": "^2.21.0"
    },
    "typings": "types.d.ts",
    "ava": {
        "require": [
            "esm"
        ],
        "files": [
            "test/**/*.test.js"
        ]
    },
    "exports": {
        "./": {
            "import": "./ems/",
            "require": "./cjs/"
        }
    },
    "prettier": {
        "tabWidth": 4,
        "singleQuote": false
    }
}
